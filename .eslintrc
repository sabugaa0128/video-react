{
  "extends": "airbnb",
  "globals": {
    "document": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "node/no-unsupported-features": "off",
    "react/jsx-filename-extension": "off",
    "no-param-reassign": "off",
    "react/prop-types": "off",
    "no-plusplus": "off",
    "comma-dangle": "off"
  }
}

